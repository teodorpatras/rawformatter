{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "plugins": ["prettier"],
  "extends": ["eslint:recommended", "airbnb/base", "prettier"],
  "rules": {
    "no-underscore-dangle": 0,
    "prefer-default-export": 0,
    "no-empty": 0,
    "class-methods-use-this": 0,
    "strict": 0,
    "global-require": 0,
    "consistent-return": 0,
    "no-restricted-globals": 0,
    "no-use-before-define": 0,
    "no-undef": 0,
    "no-param-reassign": 0,
    "arrow-body-style": 0,
    "new-cap": 0
  },
  "globals": {
    "document": true,
    "window": true,
    "console": true,
    "XMLHttpRequest": true,
    "chrome": true
  }
}
